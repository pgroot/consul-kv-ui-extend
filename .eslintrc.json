{
	"plugins": [
		"vue"
	],
	"extends": [
		"google",
		"plugin:vue/strongly-recommended"
	],
	"rules": {
		"linebreak-style": "off",
		"semi": "off",
		"no-tabs": "off",
		"comma-dangle": "off",
		"no-unused-vars": "off",
		"no-trailing-spaces": "off",
		"object-curly-spacing": "off",
		"spaced-comment": "off",
		"key-spacing": "off",
		"space-before-function-paren": "off",
		"arrow-parens": "off",
		"max-len": [
			"error",
			{
			"code": 200
			}
		],
		"keyword-spacing": "off",
		"quote-props": "off",
		"vue/html-indent": "off",
		"vue/html-self-closing": "off",
		"vue/max-attributes-per-line": "off",
		"new-cap": "off"
	}
}